<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TextViewPlus">
        <attr name="customFont" format="string"/>
    </declare-styleable>

    <color name="black">#FF000000</color>
    <color name="whitegrey">#FFF2F2F2</color>

    <style name="mydialogstyle" parent="android:style/Theme.Holo.Light.Dialog">
        <item name="android:windowNoTitle">true</item>
        <item name="@android:windowBackground">@color/whitegrey</item>
        <item name="@android:textColor">@color/black</item>
        <item name="android:datePickerStyle">@android:style/Widget.DatePicker</item>
    </style>
</resources>
